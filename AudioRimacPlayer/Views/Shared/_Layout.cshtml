<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="assets/css/bootstrap-responsive.css" rel="stylesheet">
    <link href="~/Content/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/rimac-player/RimacStyles.css?v=<?=time();?" rel="stylesheet" type="text/css"/>
    @RenderSection("styles", required: false)
    <style>
        .bsap{
            display:none!important;
        }
    </style>
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <div></div>
                @Html.ActionLink("Rimac", "Songs", "Rimac", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Song", "Songs", "Rimac")</li>
                    <li>@Html.ActionLink("Artist", "Artists", "Rimac")</li>
                    <li>@Html.ActionLink("TOP List", "TopList", "Rimac")</li>
                    <li>@Html.ActionLink("TOP", "Top", "Rimac")</li>
                    @*<li>@Html.ActionLink("Contact", "Contact", "Home")</li>*@
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()


    </div>

    <footer>
        @DateTime.Now.Year
    </footer>

   

</body>
@Scripts.Render("~/bundles/bootstrap")

@RenderSection("scripts", required: false)


</html>
